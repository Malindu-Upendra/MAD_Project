<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Available_Vehicles">

    <Spinner
        android:id="@+id/static_spinner"
        android:layout_width="321dp"
        android:layout_height="38dp"
        android:background="#887F7F"
        android:spinnerMode="dropdown"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.978"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.067"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="89dp"
        android:layout_height="84dp"
        android:background="@drawable/icon"
        android:contentDescription="@string/todo"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0">

    </ImageView>

    <TextView
        android:id="@+id/textView"
        android:layout_width="321dp"
        android:layout_height="45dp"
        android:layout_marginStart="1dp"
        android:background="#887F7F"
        android:text="@string/ceylon"
        android:textAlignment="center"
        android:textColor="#2D3775"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/imageView"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.001" />

    <SearchView
        android:id="@+id/searchView"
        android:layout_width="396dp"
        android:layout_height="31dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.173" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/available_vehicles"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.237" />

    <TableRow
        android:id="@+id/tableRow"
        android:layout_width="412dp"
        android:layout_height="49dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.333">

        <TextView
            android:id="@+id/textView6"
            android:layout_width="282dp"
            android:layout_height="match_parent"
            android:text="@string/vehicle_01"
            android:textSize="24sp" />

        <Button
            android:id="@+id/button2"
            android:layout_width="127dp"
            android:layout_height="match_parent"
            android:text="@string/view" />
    </TableRow>

    <TableRow
        android:id="@+id/tableRow2"
        android:layout_width="411dp"
        android:layout_height="54dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.403">

        <TextView
            android:id="@+id/textView8"
            android:layout_width="282dp"
            android:layout_height="match_parent"
            android:text="@string/vehicle_02"
            android:textSize="24sp" />

        <Button
            android:id="@+id/button3"
            android:layout_width="125dp"
            android:layout_height="match_parent"
            android:text="@string/view" />
    </TableRow>

    <TableRow
        android:id="@+id/tableRow4"
        android:layout_width="409dp"
        android:layout_height="53dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.482">

        <TextView
            android:id="@+id/textView9"
            android:layout_width="282dp"
            android:layout_height="match_parent"
            android:text="@string/vehicle_03"
            android:textSize="24sp" />

        <Button
            android:id="@+id/button4"
            android:layout_width="133dp"
            android:layout_height="match_parent"
            android:text="@string/view" />

    </TableRow>

    <TableRow
        android:id="@+id/tableRow3"
        android:layout_width="415dp"
        android:layout_height="57dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.568">

        <TextView
            android:id="@+id/textView10"
            android:layout_width="283dp"
            android:layout_height="match_parent"
            android:text="@string/vehicle_04"
            android:textSize="24sp" />

        <Button
            android:id="@+id/button5"
            android:layout_width="129dp"
            android:layout_height="match_parent"
            android:text="@string/view" />
    </TableRow>

    <TableRow
        android:layout_width="412dp"
        android:layout_height="61dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.656">

        <TextView
            android:id="@+id/textView11"
            android:layout_width="282dp"
            android:layout_height="match_parent"
            android:text="@string/vehicle_05"
            android:textSize="24sp" />

        <Button
            android:id="@+id/button6"
            android:layout_width="127dp"
            android:layout_height="match_parent"
            android:text="@string/view" />
    </TableRow>

</androidx.constraintlayout.widget.ConstraintLayout>



